{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@api/*": ["src/api/*"],
      "@features/*": ["src/features/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@modals/*": ["src/modals/*"],
      "@hooks/*": ["src/hooks/*"],
      "@ledget/ui": ["../../libs/ui/src"],
      "@ledget/media": ["../../libs/media/web/src"],
      "@ledget/ory": ["../../libs/ory/src"],
      "@ledget/helpers": ["../../libs/helpers/src"],
      "@ledget/shared-features": ["../../libs/shared-features/src"],
      "@ledget/media/lotties": ["../../libs/media/lotties/src"]
    }
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "extends": "../../tsconfig.base.json"
}
