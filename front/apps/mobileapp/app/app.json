{
  "expo": {
    "name": "Ledget",
    "slug": "mobileapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../../favicon_package/apple-touch-icon-180x180.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "../../favicon_package/apple-touch-icon-57x57.png",
      "bundler": "metro"
    },
    "plugins": []
  }
}
