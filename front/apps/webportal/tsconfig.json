{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "types": [
      "vite/client",
      "vitest"
    ],
    "paths": {
      "@features/*": [
        "src/features/*"
      ],
      "@pieces/*": [
        "src/pieces/*"
      ],
      "@ledget/ui": [
        "../../libs/ui/src"
      ],
      "@ledget/media": [
        "../../libs/media/src"
      ],
      "@ledget/ory": [
        "../../libs/ory/src"
      ],
    },
    "files": [],
    "include": [],
    "references": [
      {
        "path": "./tsconfig.app.json"
      },
      {
        "path": "./tsconfig.spec.json"
      }
    ],
    "extends": "../../tsconfig.base.json"
  }
}
